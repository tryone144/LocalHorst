{
    "manifest_version": 2,

    "name": "LocalHorst",
    "description": "Custom Startpage and Newtabpage",
    "author": "Bernd Busse",
    "version": "0.1",
    "icons": {
        "16": "localhorst-16.png",
        "32": "localhorst-32.png",
        "48": "localhorst-48.png",
        "128": "localhorst-128.png"
    },

    "permissions": [
        "management"
    ],
    "chrome_url_overrides": {
        "newtab": "localhorst.xhtml"
    },
    "web_accessible_resources": [
        "localhorst-32.png"
    ]
}
